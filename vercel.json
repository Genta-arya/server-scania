{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node",
        "config": { "maxPayload": "50mb" },
        "command": "node index.js && npx prisma generate"
        
        
       
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
}